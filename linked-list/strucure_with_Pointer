#include<stdio.h>
#include<stdlib.h>

struct student
{
   char *name;
   int mark;
   int rollno;

}; struct  student *p=NULL  ; //Structure  pointer variable


int main()
{

   p->name = "Hello World";
   p->mark=89;
   p->rollno=1080;
   printf("%s %d %0.2f\n",p->name,p->mark,p->rollno);
   return EXIT_SUCCESS;

}
